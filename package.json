  "description": "Progetto per il corso di ingegneria del software. Anno di corso 2021/2022",
  "main": "index.ts",
  "scripts": {
    "start": "npm run tsc && node ./out/index",
    "dev": "nodemon index.ts",
    "tsc": "tsc",
    "test-backend": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 100000 -r ts-node/register 'test/**/*.ts'",
    "test-frontend": "npm --prefix ./public/ run test -- --watchAll=false",
    "test": "npm run test-backend && npm run test-frontend",
    "lint": "eslint . --fix --ext .ts",
    "docgen": "npx typedoc --entryPointStrategy expand ./server --out ./docs"
  },
